<!-- 
Part 2: Basic BimlScript
Create five empty packages in loop
-->

<Biml xmlns="http://schemas.varigence.com/biml.xsd">
	
	<Packages>
	    
	    <# for (int number = 1; number <= 5; number++) { #>
	    
		    <Package Name="EmptyPackage<#=number#>"></Package>
		
		<# } #>
		    
	</Packages>
	
</Biml>